<?xml version="1.0" encoding="utf-8" ?>

<DataOperations xmlns="" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Imast.DataOps/DataOps.xsd">
	
	<DataConfiguration  />

	<OperationGroup Name="Products">
		
		<SqlOperation Name="GetAll" Compatibility="SqlServer" Timeout="PT5M" AutoTransaction="ReadCommitted">
			<StoredProcedure Name="GetAll" ExpectedResult="Table" />
		</SqlOperation>
	</OperationGroup>

	<OperationGroup Name="Identity">
		<SqlOperation Name="FilterGrants" Compatibility="SqlServer" Timeout="PT5M" AutoTransaction="RepeatableRead">
			<TextCommand ExpectedResult="Table">
				select * from grants
				where 1=1
				{if:useSubject { and subject = @subject } else {} }
			</TextCommand>
		</SqlOperation>
	</OperationGroup>
</DataOperations>
